def build_GRU():
  model = Sequential()
  model.add(GRU(64, input_shape=(lag, 1), return_sequences=True))
  model.add(GRU(64))
  model.add(Dropout(0.2))
  model.add(Dense(1, activation='sigmoid'))
  return model

model_struct = "GRU"
