kind: Deployment
metadata:
 name: traffic-forecasting-app
spec:
 replicas: 2
 selector:
  matchLabels:
   app: traffic-forecasting-app
 template:
  metadata:
   labels:
    app: traffic-forecasting-app
  spec:
   containers:
    - name: traffic-forecasting-app
