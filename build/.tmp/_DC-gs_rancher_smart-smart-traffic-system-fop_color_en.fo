     imagePullPolicy: Always
     ports:
      - containerPort: 5000
       protocol: TCP</fo:block></fo:block></fo:list-item-body></fo:list-item><fo:list-item id="id-1.13.4.2.2" space-before.optimum="0.8em" space-before.minimum="0.6em" space-before.maximum="1em" keep-with-next.within-column="auto" keep-with-previous.within-column="always"><fo:list-item-label end-indent="label-end()" font-family="'Open Sans', OpenSans, sans-serif" font-size="0.916em" line-height="1.8em" text-align="end" color="rgb( 12,  50,  44)" font-weight="600" background-color="inherit"><fo:block>2.</fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block text-align="inherit" line-height="1.6em" widows="3" orphans="3" margin-top="0" space-before.optimum="1em" space-before.minimum="0.8em" space-before.maximum="1.2em" color="rgb( 12,  50,  44)"><fo:inline xmlns:svg="http://www.w3.org/2000/svg" font-weight="700" background-color="inherit">service.yaml</fo:inline></fo:block></fo:block></fo:list-item-body></fo:list-item></fo:list-block><fo:block id="id-1.13.4.3" text-align="start" wrap-option="wrap" hyphenation-character="&#xA0;" font-size="8.5025pt" line-height="1.6em" space-after.minimum="inherit" orphans="2" widows="2" space-before.minimum="0.8em" space-before.optimum="1em" space-before.maximum="1.2em" space-after.optimum="1em" space-after.maximum="1.2em" hyphenate="false" white-space-collapse="false" white-space-treatment="preserve" linefeed-treatment="preserve" font-family="'DejaVu Sans Mono', DejaVuSansMono, monospace" text-transform="none" color="rgb( 12,  50,  44)" background-color="rgb(227, 248, 241)" border="none" padding="0.5em" start-indent="&#10;  inherited-property-value() + 0.5em&#10;" end-indent="&#10;  inherited-property-value() + 0.5em&#10;">apiVersion: v1
